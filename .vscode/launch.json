{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"preLaunchTask": "tsc: watch - tsconfig.json",
			"outFiles": [
                "${workspaceRoot}/out/src/**/*.js"
            ],
			"runtimeExecutable": "${execPath}",
			"sourceMaps": true,
			"args": [
				"--profile-temp",
				"--extensionDevelopmentPath=${workspaceFolder}"
			]
		}
	]
}
